<%- include('./partial/header')%>

<div class="mt-5">
<form action="/book/edit/<%= data.id%>" method="POST">
    <div class="form-group">
        <label for="exampleInputPassword1">Title</label>
        <input type="text" class="form-control" name="title" value="<%= data.title%>">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Writer</label>
        <input type="text" class="form-control" name="writer" value="<%= data.writer%>">
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Publisher</label>
        <input type="text" class="form-control" name="publisher" value="<%= data.publisher%>">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Price</label>
        <input type="number" class="form-control" name="price" value="<%= data.price%>">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Released Year</label>
        <input type="number" class="form-control" name="releasedyear" value="<%= data.released_year%>">
